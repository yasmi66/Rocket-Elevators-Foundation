<p id="notice"><%= notice %></p>

<h1>Elevators</h1>

<table>
  <thead>
    <tr>
      <th>Column</th>
      <th>Serial nb</th>
      <th>Model</th>
      <th>Elevator Status</th>
      <th>Date commissioning</th>
      <th>Date last inspection</th>
      <th>Certificate inspection</th>
      <th>Information</th>
      <th>Notes</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @elevators.each do |elevator| %>
      <tr>
        <td><%= elevator.column %></td>
        <td><%= elevator.serial_nb %></td>
        <td><%= elevator.model %></td>
        <td><%= elevator.elevator_status %></td>
        <td><%= elevator.date_commissioning %></td>
        <td><%= elevator.date_last_inspection %></td>
        <td><%= elevator.certificate_inspection %></td>
        <td><%= elevator.information %></td>
        <td><%= elevator.notes %></td>
        <td><%= link_to 'Show', elevator %></td>
        <td><%= link_to 'Edit', edit_elevator_path(elevator) %></td>
        <td><%= link_to 'Destroy', elevator, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Elevator', new_elevator_path %>
